{
  "name": "clean-arch-express",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "tsx watch src/main/server.ts",
    "build": "tsc",
    "star": "node dist/main/server.js",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:seed": "tsx src/scripts/prisma/seed/seed.ts",
    "docker:build": "docker-compose -f docker-compose.yml build",
    "docker:up": "docker-compose -f docker-compose.yml up -d",
    "docker:down": "docker-compose -f docker-compose.yml down",
    "docker:stop": "docker-compose -f docker-compose.yml stop",
    "docker:restart": "docker-compose -f docker-compose.yml restart",
    "docker:logs": "docker-compose -f docker-compose.yml logs -f",
    "docker:ps": "docker-compose -f docker-compose.yml ps",
    "docker:exec": "docker-compose -f docker-compose.yml exec api bash",
    "docker:db:migrate": "docker-compose exec db prisma migrate dev",
    "docker:db:generate": "docker-compose exec db prisma generate",
    "docker:db:studio": "docker-compose exec db prisma studio",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.2.0",
    "@vitest/coverage-v8": "^3.2.4",
    "prisma": "^6.13.0",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@prisma/client": "^6.13.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "google-auth-library": "^10.5.0",
    "jsonwebtoken": "^9.0.2",
    "zod": "^4.0.15"
  }
}
